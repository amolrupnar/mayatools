import time
from multiprocessing import Process as psp


class Foo(object):
    def __init__(self):
        self.a = 'amol'

    def printTime(self):
        for each in range(10):
            self.a = each
            time.sleep(1)

    def another_function(self):
        print self.a


if __name__ == '__main__':
    f1 = Foo()
    p1 = psp(target=f1.printTime())
    p1.start()
    p2 = psp(target=f1.another_function())
    p2.start()
